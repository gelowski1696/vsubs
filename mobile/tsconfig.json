{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "rxjs": ["../node_modules/rxjs"],
      "rxjs/*": ["../node_modules/rxjs/*"],
      "@angular/*": ["../node_modules/@angular/*"]
    },
    "strict": true,
    "skipLibCheck": true,
    "types": ["leaflet"]
  },
  "files": ["src/main.ts"],
  "include": ["src/**/*.ts", "src/**/*.d.ts"]
}
